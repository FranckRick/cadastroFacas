
 <link rel="stylesheet" href='/css/message.css'>
 <link
 href="https://fonts.googleapis.com/css2?family=DM+Sans:wght@400;700&display=swap"
 rel="stylesheet"
/>

<% if (errors.length > 0) { %>
  <div class="row">
    <div class="col my-3">
      <div class="alert alert-danger">
        <% errors.forEach(erro => { %>
        <%= erro %><br>
        <% }); %>
      </div>
    </div>
  </div>
  <% } %>
  
  <% if (success.length > 0) { %>
  <div class="row">
    <div class="col my-3">
      <div class="alert alert-success">
        <% success.forEach(success => { %>
        <%= success %><br>
        <% }); %>
      </div>
    </div>
  </div>
  <% } %>